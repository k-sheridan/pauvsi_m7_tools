function [ p1, p2 ] = trajectoryPlotter( Traj )
%UNTITLED Summary of this function goes here
%   plots the trajectory

[trajRow, trajCol, trajZ] = size(Traj)

time = 0;

p1 = [polyval(Traj(1, 1:10, 1), (time:0.1:time + Traj(1, 11, 1)))', polyval(Traj(2, 1:10, 1), (time:0.1:time + Traj(1, 11, 1)))', polyval(Traj(3, 1:10, 1), (time:0.1:time + Traj(1, 11, 1)))'];
p2 = [polyval(Traj(1, 1:10, 1), (time + 0.1:0.1:time + Traj(1, 11, 1)))', polyval(Traj(2, 1:10, 1), (time + 0.1:0.1:time + Traj(1, 11, 1)))', polyval(Traj(3, 1:10, 1), (time + 0.1:0.1:time + Traj(1, 11, 1)))'];
time = time + Traj(1, 11, 1);

for z_index = (2:1:trajZ)
    p1 = [p1;[polyval(Traj(1, 1:10, z_index), (0:0.1:time + Traj(1, 11, z_index)))', polyval(Traj(2, 1:10, z_index), (0:0.1:time + Traj(1, 11, z_index)))', polyval(Traj(3, 1:10, z_index), (0:0.1:time + Traj(1, 11, z_index)))'];
    p2 = [p2; [polyval(Traj(1, 1:10, z_index), (0.1:0.1:time + Traj(1, 11, z_index)))', polyval(Traj(2, 1:10, z_index), (0.1:0.1:time + Traj(1, 11, z_index)))', polyval(Traj(3, 1:10, z_index), (0.1:0.1:time + Traj(1, 11, z_index)))'];
    time = time + Traj(1, 11, z_index);
end

end

